nts\\Products.js",["94"],[],"import React, { useState, useEffect } from \"react\";\r\nimport Skeleton from \"react-loading-skeleton\";\r\nimport { NavLink } from \"react-router-dom\";\r\n\r\nconst Products = () => {\r\n  const [data, setData] = useState([]);\r\n  const [filter, setFilter] = useState(data);\r\n  const [loading, setLoading] = useState(false);\r\n  let componentMounted = true;\r\n  useEffect(() => {\r\n    const getProducts = async () => {\r\n      setLoading(true);\r\n      const response = await fetch(\"https://fakestoreapi.com/products\");\r\n\r\n      if (componentMounted) {\r\n        setData(await response.clone().json());\r\n        setFilter(await response.json());\r\n        setLoading(false);\r\n        console.log(filter);\r\n      }\r\n      return () => {\r\n        componentMounted = false;\r\n      };\r\n    };\r\n    getProducts();\r\n  }, []);\r\n  const Loading = () => {\r\n    return (\r\n      <>\r\n        <div className=\"col-md-3\">\r\n          <Skeleton height={350} />\r\n        </div>\r\n        <div className=\"col-md-3\">\r\n          <Skeleton height={350} />\r\n        </div>\r\n        <div className=\"col-md-3\">\r\n          <Skeleton height={350} />\r\n        </div>\r\n        <div className=\"col-md-3\">\r\n          <Skeleton height={350} />\r\n        </div>\r\n      </>\r\n    );\r\n  };\r\n  const filterProducts=(cat)=>{\r\n    const updatedList=data.filter((x)=>x.category===cat);\r\n    setFilter(updatedList);\r\n  }\r\n  const ShowProducts = () => {\r\n    return (\r\n      <>\r\n        <div className=\"buttons d-flex justify-content-center mb-2 pb-5\">\r\n          <button\r\n            className=\"btn btn-outline-dark me-2 \"\r\n            onClick={() => setFilter(data)}\r\n          >\r\n            All\r\n          </button>\r\n          <button\r\n            className=\"btn btn-outline-dark me-2\"\r\n            onClick={() => filterProducts(\"men's clothing\")}\r\n          >\r\n            Men's Clothing\r\n          </button>\r\n          <button className=\"btn btn-outline-dark me-2 \"  onClick={() => filterProducts(\"women's clothing\")}>\r\n            Women's Clothing\r\n          </button>\r\n          <button className=\"btn btn-outline-dark me-2\" onClick={() => filterProducts(\"jewelery\")}>\r\n            Jewelery's \r\n          </button>\r\n          <button className=\"btn btn-outline-dark me-2\" onClick={() => filterProducts(\"electronics\")}>Electronic</button>\r\n        </div>\r\n        {filter.map((product) => {\r\n          return (\r\n            <>\r\n              <div className=\"col-md-3 mb-2\">\r\n                <div className=\"card h-100 text-center p-4 key={product.id}\">\r\n                  <img\r\n                    src={product.image}\r\n                    className=\"card-img-top\"\r\n                    alt=\"{product.title}\"\r\n                    height=\"250px\"\r\n                  />\r\n                  <div className=\"card-body\">\r\n                    <h5 className=\"card-title mb-0\">\r\n                      {product.title.substring(0, 12)}...\r\n                    </h5>\r\n                    <p className=\"card-text lead fw-bold\">${product.price}</p>\r\n                    <NavLink to={`/product/${product.id}`} className=\"btn btn-outline-dark\">\r\n                      Buy Now\r\n                    </NavLink>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </>\r\n          );\r\n        })}\r\n      </>\r\n    );\r\n  };\r\n  return (\r\n    <div>\r\n      <div className=\"container my-3 py-5\">\r\n        <div className=\"row\">\r\n          <div className=\"col-12 mb-3\">\r\n            <h1 className=\"display-6 fw-bolder text-center\">Latest Products</h1>\r\n            <hr />\r\n          </div>\r\n        </div>\r\n        <div className=\"row justify-content-center\">\r\n          {loading ? <Loading /> : <ShowProducts />}\r\n        </div>\r\n      </d